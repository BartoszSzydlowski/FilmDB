@{
	ViewData["Title"] = "Films";
}

<table>
	  <tr>
		  <th>Id</th>
		  <th>Title</th>
		  <th>Genre</th>
		  <th>Year</th>
	  </tr>
	@foreach (var film in ViewBag.Films)
	{
		<tr>
			<td>@film.Id</td>
			<td>@film.Title</td>
			<td>@film.Genre</td>
			<td>@film.Year</td>
		</tr>
	}
</table>